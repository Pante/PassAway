<div class="form-group col-xs-12">
    <label for="inputEmail3" class="col-xs-2">Email</label>
    <input type="email" class="form-control col-xs-10" id="inputEmail3" placeholder="Email" required>
</div>
<div class="form-group col-xs-12">
    <label for="inputPassword3" class="col-xs-2">Password</label>
    <input type="password" class="form-control col-xs-10" id="inputPassword3" placeholder="Password" required>
</div>
<div class="form-group col-xs-12">
    <label for="inputUsername3" class="col-xs-2">Password</label>
    <input type="text" class="form-control col-xs-10" id="inputUsername3" placeholder="Username" required>
</div>
<div class="form-group col-xs-12">
    <label for="inputRePassword3" class="col-xs-2">Re-Password</label>
    <input type="password" class="form-control col-xs-10" id="inputRePassword3" placeholder="Re-Password" required>
</div>
<div class="form-group">
    <div class="col-xs-offset-2 col-xs-10">
        <div class="radio">
            <label>
                <input type="checkbox"> Male
            </label>
            <label>
                <input type="checkbox"> Female
            </label>
        </div>
    </div>
</div>
<div class="form-group col-xs-12">
    <div class="col-xs-offset-2 col-xs-10">
        <button type="submit" class="btn btn-default">Register</button>
    </div>
</div> 